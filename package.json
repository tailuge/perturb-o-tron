{
  "name": "perturb-o-tron",
  "version": "1.0.0",
  "description": "explore endgames by perturbing pieces",
  "main": "dist/main.js",
  "author": "tailuge",
  "license": "GPL-3.0",
  "scripts": {
    "test": "nyc mocha --require ts-node/register -r jsdom-global/register ./test/*.spec.ts",
    "watch": "gulp",
    "prettify": "prettier --write --no-semi {src,test}/*",
    "deps": "npx yarn-check --skip-unused"
  },
  "dependencies": {
    "chess.js": "^0.10",
    "chessground": "^7.3",
    "search-params": "^2.1.3"
  },
  "devDependencies": {
    "@types/mocha": "5.2.7",
    "browserify": "16.5.0",
    "chai": "4.2.0",
    "coveralls": "3.0.9",
    "gulp": "4.0.2",
    "gulp-mocha": "^7.0.2",
    "gulp-sourcemaps": "2.6.5",
    "gulp-typescript": "6.0.0-alpha.1",
    "gulp-uglify": "3.0.2",
    "gulp-util": "^3",
    "jsdom": "15.2.1",
    "jsdom-global": "3.0.2",
    "mocha": "^6.2.2",
    "nyc": "^12.0.2",
    "prettier": "1.19.1",
    "sinon": "^8.0.1",
    "ts-node": "8.5.4",
    "tsify": "4.0.1",
    "typescript": "3.7.4",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "3.11.1"
  },
  "nyc": {
    "include": [
      "./src/*.ts"
    ],
    "exclude": [
      ".test/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
